{
  "name": "@0xff336699/js-i18n-lngs",
  "version": "1.0.29",
  "description": "i18n lngs",
  "private": true,
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./src/index.ts",
  "files": [
    "dist/**",
    "src/**"
  ],
  "scripts": {
    "clean:bundle": "pnpx rimraf dist",
    "clean:node_modules": "pnpx rimraf node_modules",
    "clean:turbo": "pnpx rimraf .turbo",
    "clean": "pnpm clean:bundle && pnpm clean:node_modules && pnpm clean:turbo",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build": "pnpm clean:node_modules && pnpm build:esm && pnpm build:cjs",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write --ignore-path ../../.prettierignore",
    "type-check": "tsc --noEmit",
    "build2": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2",

    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/jest": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-loader": "^8.0.12",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0"
  }
}
